pipeline{
    agent any
    stages{
    stage('SonarQube analusis'){
            
        steps{
        withSonarQubeEnv('sonarcicd')
        {
        withMaven(maven:'mavenci')
        {
        bat "mvn sonar:sonar"
        }    
        }    
        }
        }
        stage('maven build')
        {
        steps
        {
        withMaven(maven:'mavenci')
        {
        bat "mvn clean install"
        }
        }
        }
        
    }
}
